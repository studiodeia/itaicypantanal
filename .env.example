# Core DB (required for migrations, metrics and panel user persistence)
DATABASE_URL=postgresql://USER:PASSWORD@HOST:5432/DB

# Panel auth
PANEL_JWT_SECRET=change-this-secret-with-at-least-16-chars
# Optional fallback accounts (used when DATABASE_URL is unavailable)
# PANEL_ACCOUNTS_JSON=[{"username":"admin","password":"admin123","role":"admin","displayName":"Administrador","enabled":true}]
PANEL_TOKEN_TTL_SEC=28800

# Agent model runtime (Vercel AI SDK)
AGENT_MODEL=claude-3-5-sonnet-latest
AGENT_PROVIDER=auto
AGENT_TEMPERATURE=0.35
AGENT_MAX_OUTPUT_TOKENS=900
AGENT_MAX_OUTPUT_TOKENS_COMMERCIAL=540
AGENT_MAX_OUTPUT_TOKENS_POLICY=620
AGENT_FIRST_TOKEN_DELAY_MS=220
# Configure one provider key (or both, with AGENT_PROVIDER=auto)
# OPENAI_API_KEY=
# ANTHROPIC_API_KEY=

# Optional admin key for internal agent routes
# AGENT_ADMIN_KEY=your-admin-key

# Cloudbeds OAuth/API
CLOUDBEDS_ENABLED=false
CLOUDBEDS_API_BASE_URL=https://api.cloudbeds.com/api/v1.3
CLOUDBEDS_OAUTH_TOKEN_URL=https://api.cloudbeds.com/api/v1.3/access_token
CLOUDBEDS_CLIENT_ID=
CLOUDBEDS_CLIENT_SECRET=
CLOUDBEDS_OAUTH_AUTHORIZE_URL=https://api.cloudbeds.com/api/v1.3/oauth
CLOUDBEDS_OAUTH_REDIRECT_URI=https://www.itaicypantanal.com.br/api/agent/cloudbeds/oauth/callback
# Optional routing defaults for Cloudbeds tools
# CLOUDBEDS_AVAILABILITY_PATH=/getAvailableRoomTypes
# CLOUDBEDS_RATES_PATH=/getRatePlans
# CLOUDBEDS_PROPERTY_IDS=12345
# CLOUDBEDS_DEFAULT_ROOMS=1
# For callback grant exchange:
# - authorization_code (OAuth padrao)
# - urn:ietf:params:oauth:grant-type:api-key (alternative OAuth/API key delivery)
CLOUDBEDS_OAUTH_CALLBACK_GRANT_TYPE=authorization_code
# Optional after first callback exchange
# CLOUDBEDS_REFRESH_TOKEN=
# CLOUDBEDS_ACCESS_TOKEN=
