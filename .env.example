# Core DB (required for migrations, metrics and panel user persistence)
DATABASE_URL=postgresql://USER:PASSWORD@HOST:5432/DB

# Panel auth
PANEL_JWT_SECRET=change-this-secret-with-at-least-16-chars
# Optional fallback accounts (used when DATABASE_URL is unavailable)
# PANEL_ACCOUNTS_JSON=[{"username":"admin","password":"admin123","role":"admin","displayName":"Administrador","enabled":true}]
PANEL_TOKEN_TTL_SEC=28800

# Agent model runtime (Vercel AI SDK)
AGENT_MODEL=claude-3-5-sonnet-latest
AGENT_TEMPERATURE=0.35
AGENT_MAX_OUTPUT_TOKENS=900
AGENT_MAX_OUTPUT_TOKENS_COMMERCIAL=540
AGENT_MAX_OUTPUT_TOKENS_POLICY=620
AGENT_FIRST_TOKEN_DELAY_MS=220

# Optional admin key for internal agent routes
# AGENT_ADMIN_KEY=your-admin-key
